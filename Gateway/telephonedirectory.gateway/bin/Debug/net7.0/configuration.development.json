{
  "Routes": [
    {
      "UseServiceDiscovery": true,
      "ServiceName": "ContactService",
      "DownstreamPathTemplate": "/api/v1/Contact/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/services/Contact/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "SwaggerKey": "contact"
    },
    {
      "UseServiceDiscovery": true,
      "ServiceName": "ReportService",
      "DownstreamPathTemplate": "/api/v1/Report/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/services/Report/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE" ],
      "SwaggerKey": "report"
    } 
  ],
  "SwaggerEndPoints": [
    {
      "Key": "contact",
      "Config": [
        {
          "Name": "Contact",
          "Version": "v1",
          "Url": "http://localhost:7101/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "report",
      "Config": [
        {
          "Name": "Report",
          "Version": "v1",
          "Url": "http://localhost:7102/swagger/v1/swagger.json"
        }
      ]
    } 
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7100",
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}